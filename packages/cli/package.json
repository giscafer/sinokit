{
  "version": "0.2.14",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "templates"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "cli",
  "author": "giscafer",
  "module": "dist/cli.esm.js",
  "size-limit": [
    {
      "path": "dist/cli.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/cli.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.6.0",
    "@types/fs-extra": "^9.0.2",
    "@types/sade": "^1.7.2",
    "@types/semver": "^7.3.4",
    "@types/shelljs": "^0.8.8",
    "husky": "^4.3.0",
    "size-limit": "^4.6.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "ansi-escapes": "^4.3.1",
    "chalk": "^4.1.0",
    "download-git-repo": "^3.0.2",
    "enquirer": "^2.3.6",
    "execa": "^4.0.3",
    "fs-extra": "^9.0.1",
    "ora": "^5.1.0",
    "sade": "^1.7.4",
    "semver": "^7.3.2",
    "shelljs": "^0.8.4"
  }
}
